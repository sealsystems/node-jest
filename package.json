{
  "name": "@sealsystems/test-config-jest",
  "version": "1.0.0",
  "description": "Test configuration for Jest at SEAL Systems",
  "author": {
    "name": "SEAL Systems AG",
    "email": "opensource@sealsystems.de",
    "url": "https://www.sealsystems.de"
  },
  "license": "MIT",
  "bin": {
